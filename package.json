{
  "name": "biee-ui",
  "private": false,
  "version": "0.0.7",
  "type": "module",
  "description": "A reusable React component, powered by Vite and Node! This component is designed to streamline your React development process.",
  "license": "MIT",
  "author": "Samuel Peters",
  "keywords": [],
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/biee-ui.umd.js",
  "module": "./dist/biee-ui.es.js",
  "exports": {
    ".": {
      "import": "./dist/biee-ui.es.js",
      "require": "./dist/biee-ui.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest run",
    "watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@testing-library/dom": "^9.2.0",
    "@testing-library/react": "^14.0.0",
    "biee-ui": "^0.0.6",
    "c8": "^7.13.0",
    "jsdom": "^21.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "react-test-renderer": "^18.2.0",
    "vitest": "^0.30.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.39.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.3.2",
    "vite": "^4.3.3"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es2020": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bieefilled/Library.git"
  },
  "bugs": {
    "url": "https://github.com/bieefilled/Library/issues"
  },
  "Contribute": {
    "url": "https://github.com/bieefilled/Library/CONTRIBUTION.md"
  },
  "homepage": "https://github.com/bieefilled/Library#readme"
}
